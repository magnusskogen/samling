<!DOCTYPE html>
<html
  class="no-js {{ bgColor }}{% if bodyClass %} {{ bodyClass }}{% endif %}"
  lang="{{ shop.locale }}"
>
  <head>
    <meta name="viewport" content="width=device-width,initial-scale=1" />
    <title>
      {{ page_title }}{% if current_tags %} &ndash; tagged "{{
        current_tags | join: ", "
      }}"{% endif %}{% if current_page != 1 %} &ndash; Page {{ current_page }}{%
      endif %}{% unless page_title contains 'Samling' %}{% endunless %}
    </title>
    {% include 'seo' %}

    <link rel="icon" type="image/png" href="{{ 'favicon2.png' | asset_url }}" />

    <link rel="canonical" href="{{ canonical_url }}" />

    <link href="{{ 'styles.css' | asset_url }}" rel="stylesheet" type="text/css" media="all" />

    {{ content_for_header  }}
  <link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=Poppins:wght@500&display=swap" rel="stylesheet">
  </head>

  <body data-barba="wrapper" class="{{ invert }} w-screen relative overflow-x-hidden">



    <div data-barba="container" class="relative w-screen overflow-x-hidden">
      <div class="main-container flex flex-col">
        <div class="w-full max-w-2xl mx-auto bg-pink lg:my-12 relative">
          {% include 'header' %}
          {{ content_for_layout }}
          {% include 'footer' %}
        </div>
      </div>
   </div>

    <!--<script type="text/javascript" src="{{ 'bundle.js' | asset_url }}"></script>-->
    <script type="text/javascript" src="{{ 'bundle2.js' | asset_url }}"></script>

  {% render 'th-subscription-scripts' %} {% include 'appstle-subscription-helper' %}</body>
</html>
